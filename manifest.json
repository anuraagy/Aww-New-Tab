{
"update_url": "https://clients2.google.com/service/update2/crx",

    "manifest_version": 2,
    "name": "Aww",
    "description": "Replaces the new tab page with the cutest gifs from /r/aww",
    "version": "1.1",
    "permissions": [
        "tabs",
        "http://reddit.com/*"
    ],
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
    }
}
